{
    "extends": "airbnb/base",
    "rules": {
        "dot-notation": 0,
        "id-length": 0,
        "indent": [2, 4],
        "no-param-reassign": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": [2, {"vars": "all", "args": "none"}],
        "no-use-before-define": 0,
        "object-curly-spacing": 0,
        "quotes": 0,
        "space-infix-ops": 0,
        "spaced-comment": 0
    }
}
